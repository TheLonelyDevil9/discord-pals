{
  "providers": [
    {
      "name": "OpenAI",
      "url": "https://api.openai.com/v1",
      "key_env": "OPENAI_API_KEY",
      "model": "gpt-4o",
      "max_tokens": 4096,
      "temperature": 1.0
    },
    {
      "name": "Anthropic (via OpenRouter)",
      "url": "https://openrouter.ai/api/v1",
      "key_env": "OPENROUTER_API_KEY",
      "model": "anthropic/claude-3.5-sonnet",
      "max_tokens": 8192,
      "temperature": 1.0
    },
    {
      "name": "Local LLM (LM Studio/Ollama/llama.cpp)",
      "url": "http://localhost:1234/v1",
      "key_env": "LOCAL_API_KEY",
      "model": "local-model",
      "max_tokens": 2048,
      "temperature": 0.8,
      "requires_key": false,
      "include_body": "min_p: 0.1\ntop_k: 40\nrepetition_penalty: 1.1",
      "exclude_body": "- frequency_penalty\n- presence_penalty"
    }
  ],
  "timeout": 60
}
